{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "baseUrl": ".",
    "emitDecoratorMetadata": false,
    "esModuleInterop": true,
    "experimentalDecorators": false,
    "importHelpers": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false,
    "outDir": "./dist",
    "noFallthroughCasesInSwitch": false,
    "noImplicitAny": true,
    "noImplicitReturns": false,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "pretty": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "isolatedModules": true,
    "strict": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "types": ["node", "jest"],
    "typeRoots": ["./node_modules/@types", "./types"],
    "paths": {
      ".storybook": ["./.storybook"],
      "@rarui/scripts": ["./packages/core/scripts/src"],
      "@rarui/webpack": ["./packages/core/webpack/src"],
      "@rarui/typings": ["./packages/core/typings/src"],
      "@rarui/styles": ["./packages/styles/src"],
      "@rarui/tokens": ["./packages/tokens"],
      "@rarui/icons": ["./packages/icons"],
      "@rarui-react/avatar": [
        "./packages/implementations/react/src/atomic/Avatar/src"
      ],
      "@rarui-react/badge": [
        "./packages/implementations/react/src/atomic/Badge/src"
      ],
      "@rarui-react/banner": [
        "./packages/implementations/react/src/atomic/Banner/src"
      ],
      "@rarui-react/box": [
        "./packages/implementations/react/src/atomic/Box/src"
      ],
      "@rarui-react/button": [
        "./packages/implementations/react/src/atomic/Button/src"
      ],
      "@rarui-react/checkbox": [
        "./packages/implementations/react/src/atomic/Checkbox/src"
      ],
      "@rarui-react/chip": [
        "./packages/implementations/react/src/atomic/Chip/src"
      ],
      "@rarui-react/icon": [
        "./packages/implementations/react/src/atomic/Icon/src"
      ],
      "@rarui-react/icon-button": [
        "./packages/implementations/react/src/atomic/IconButton/src"
      ],
      "@rarui-react/input": [
        "./packages/implementations/react/src/atomic/Input/src"
      ],
      "@rarui-react/label": [
        "./packages/implementations/react/src/atomic/Label/src"
      ],
      "@rarui-react/link": [
        "./packages/implementations/react/src/atomic/Link/src"
      ],
      "@rarui-react/radio-button": [
        "./packages/implementations/react/src/atomic/RadioButton/src"
      ],
      "@rarui-react/skeleton": [
        "./packages/implementations/react/src/atomic/Skeleton/src"
      ],
      "@rarui-react/status": [
        "./packages/implementations/react/src/atomic/Status/src"
      ],
      "@rarui-react/text": [
        "./packages/implementations/react/src/atomic/Text/src"
      ],
      "@rarui-react/title": [
        "./packages/implementations/react/src/atomic/Title/src"
      ],
      "@rarui-react/toggle": [
        "./packages/implementations/react/src/atomic/Toggle/src"
      ],
      "@rarui-react/toast": [
        "./packages/implementations/react/src/atomic/Toast/src"
      ],
      "@rarui-react/tooltip": [
        "./packages/implementations/react/src/atomic/Tooltip/src"
      ],
      "@rarui-vuejs/button": [
        "./packages/implementations/vuejs/src/atomic/Button/src"
      ],
      "@rarui-react/divider": [
        "./packages/implementations/react/src/atomic/Divider/src"
      ]
    }
  },
  "include": ["packages", "jest.setup.ts"]
}
